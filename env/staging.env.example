# Spring profile to activate
SPRING_PROFILES_ACTIVE=staging

# Application metadata
APP_NAME="Spring Boot Boilerplate (Staging)"
SERVER_PORT=8081
SERVER_CONTEXT_PATH=/

# Database connectivity
STAGING_DB_URL=jdbc:postgresql://staging-db:5432/app_staging
STAGING_DB_USERNAME=staging_user
STAGING_DB_PASSWORD=staging_password

# Logging
LOG_FILE=logs/application-staging.log
