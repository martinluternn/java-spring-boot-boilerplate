# Spring profile to activate
SPRING_PROFILES_ACTIVE=production

# Application metadata
APP_NAME="Spring Boot Boilerplate"
SERVER_PORT=8080
SERVER_CONTEXT_PATH=/

# Database connectivity
PROD_DB_URL=jdbc:postgresql://prod-db:5432/app_production
PROD_DB_USERNAME=prod_user
PROD_DB_PASSWORD=prod_password

# Logging
LOG_FILE=logs/application-production.log
